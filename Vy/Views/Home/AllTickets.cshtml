@model IEnumerable<Vy.Models.Ticket>


<h2>All Tickets</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TicketPrice)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TicketDuration)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartStation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndStation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TicketPurchaseDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TicketPurchaseTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrderedBy)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TicketPrice)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TicketDuration)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartStation)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EndStation)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TicketPurchaseDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TicketPurchaseTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrderedBy)
        </td>
        <td>
            @Html.ActionLink("Details","getReceipt", "Receipt", new { id=item.TicketID },null)
        </td>
    </tr>
}

</table>
